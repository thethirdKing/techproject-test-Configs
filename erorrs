# Clear any existing DHCP bindings and conflicts
clear ip dhcp binding *
clear ip dhcp conflict *

# Configure a static IP temporarily
interface GigabitEthernet0/3
 no ip address dhcp
 ip address 192.168.255.2 255.255.255.0
 no shutdown

# Test connectivity to the DHCP server or default gateway
ping 192.168.255.1
