configure terminal
interface GigabitEthernet1/2
 no shutdown
 no ip address dhcp
 ip address 10.150.127.9 255.255.255.252
 ip ospf 15 area 0
 no passive-interface
exit
interface GigabitEthernet1/3
 no shutdown
 no ip address dhcp
 ip address 10.150.127.17 255.255.255.252
 ip ospf 15 area 0
 no passive-interface
exit
router ospf 15
 network 10.150.127.0 0.0.0.255 area 0
end
